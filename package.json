{
  "name": "ngx-defi-core",
  "version": "1.2.14",
  "scripts": {
    "coverage": "ng test --watch=false --code-coverage",
    "linkCoverageWithDoc": "node documentation/addlinkcoverage.js",
    "test": "ng test",
    "compodoc": "compodoc -p src/tsconfig.app.json",
    "doc": "npm run compodoc && npm run coverage && npm run linkCoverageWithDoc && npm run badge && git add documentation && git commit -m 'update documentation'",
    "start": "ng serve",
    "autoExport": "node autoExport",
    "packagr": "ng-packagr -p ng-package.json",
    "badge": "node badge.js",
    "patch": "npm run doc && npm run autoExport && npm run packagr && git add . && git commit -m 'patch' && npm version patch && git push origin master --follow-tags",
    "minor": "npm run doc && npm run autoExport && npm run packagr && git add . && git commit -m 'minor' && npm version minor && git push origin master --follow-tags",
    "major": "npm run doc && npm run autoExport && npm run packagr && git add . && git commit -m 'major' && npm version major && git push origin master --follow-tags"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@ng-bootstrap/ng-bootstrap": "^2.0.0",
    "angular-playground": "^4.0.1",
    "angular2-csv": "^0.2.5",
    "bootstrap": "^4.1.1",
    "core-js": "^2.5.6",
    "element-resize-detector": "^1.1.14",
    "ngx-bootstrap": "^2.0.5",
    "rxjs": "^6.0.0",
    "rxjs-compat": "^6.1.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular/cli": "~6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/language-service": "^6.0.0",
    "@compodoc/compodoc": "^1.1.2",
    "@types/element-resize-detector": "^1.1.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "events-stream": "^0.2.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.5",
    "karma-spec-reporter": "0.0.32",
    "lcov2badge": "^0.1.0",
    "ng-packagr": "^2.4.4",
    "node-sass": "^4.9.0",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
